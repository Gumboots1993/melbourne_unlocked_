          <div class="unlockedtop-two">
          <%= link_to cl_image_tag(@top_three.photo.key, class: "avatar mb-1") , profile_path(@top_three.username) %>
          <p><%= @top_three.username %> <i class="fas fa-unlock-alt"></i> <strong><%= @top_three.visits.count %></strong></p>

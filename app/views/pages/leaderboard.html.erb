<div class="desktop-container">
<div class="leader_part2">
  <div class="container">
    <div id="leader" class="cards">
    <br>
      <% @top_ten.each do |user| %>
        <div id="leader" class="card mt-1 mb-1">
          <div class="section">
            <%= link_to cl_image_tag(user.photo.key, class: "avatar") , profile_path(user.username) %>
          </div>
          <div class="section mt-auto mb-auto">
              <h3><%= link_to user.username , profile_path(user.username), class: "text-decoration-none"%></h3>
              <p><strong> <i class="fas fa-unlock-alt"></i>   Total Unlocked: </strong> <%= user.visits.count %> /
              <%= @locks.count %> </p>
          </div>
          <div class="section ms-auto mb-auto mt-auto pe-3">
          <%= "#{@counter += 1}th"  %>
          </div>
        </div>
        <div class="line border-bottom" style="width: 90%"></div>
      <% end %>
      <br>
    </div>
  </div>
